{
  "name": "what-is-project-tech-stack",
  "version": "0.1.0",
  "description": "CLI tool that analyzes project dependencies and generates human-readable tech stack descriptions",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "what-is-my-tech-stack": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli/index.js",
    "dev": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install"
  },
  "keywords": [
    "tech-stack",
    "dependencies",
    "analyzer",
    "cli",
    "openai",
    "project-analysis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^22.13.5",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "openai": "^4.85.4",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.21.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.5.2",
    "ts-jest": "^29.2.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
